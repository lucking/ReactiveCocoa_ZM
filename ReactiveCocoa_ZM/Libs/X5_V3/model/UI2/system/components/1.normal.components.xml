<?xml version="1.0" encoding="UTF-8"?>
<reg-info>
	<!-- 组件目录注册 -->
	<component-dirs>
		default,html,justep/operation,justep/model,justep/data,justep/window,justep/input,justep/output,justep/select,justep/textarea,justep/button,
		justep/tabs,justep/panel, justep/popOver, justep/popMenu, justep/menu,
		justep/wing, justep/widgets, justep/contents,
		justep/scrollView,justep/titleBar,justep/bar,justep/windowContainer,justep/messageDialog,
		justep/dialog,justep/windowDialog,justep/windowReceiver,justep/list,justep/labelEdit,justep/controlGroup,justep/calendar,
		justep/gridLayout,justep/row,
		justep/cordova,justep/cellLayout,justep/excelLayout,justep/absoluteLayout
	</component-dirs>

	<!-- 组件工具箱配置 -->
	<toolbox>

		<catalog name="数据" order="1">
			<item component-name="$UI/system/components/justep/model/model" />
			<item component-name="$UI/system/components/justep/data/data" />
		</catalog>

		<catalog name="布局" order="2">
			<item component-name="$UI/system/components/justep/contents/contents" />
			<item component-name="$UI/system/components/justep/panel/panel" />
			<item component-name="$UI/system/components/justep/titleBar/titleBar" />
			<item component-name="$UI/system/components/justep/bar/bar" />
			<item component-name="$UI/system/components/justep/scrollView/scrollView" />
			<item component-name="$UI/system/components/justep/row/row" />
			<item component-name="$UI/system/components/justep/popOver/popOver" />
			<item component-name="$UI/system/components/justep/popMenu/popMenu" />
			<item component-name="$UI/system/components/justep/wing/wing" />
			<item component-name="$UI/system/components/justep/widgets/widgets" />
			<item component-name="$UI/system/components/justep/windowContainer/windowContainer" />
			<item component-name="$UI/system/components/justep/cellLayout/cellLayout" />
			<item component-name="$UI/system/components/justep/excelLayout/excelLayout" />
			<item component-name="$UI/system/components/justep/absoluteLayout/absoluteLayout" />

		</catalog>

		<catalog name="表单" initial-state="open" design-view="excel"
			order="3">
			<item component-name="$UI/system/components/justep/input/input" />
			<item component-name="$UI/system/components/justep/input/password" />
			<item component-name="$UI/system/components/justep/input/range" />
			<item component-name="$UI/system/components/justep/output/output" />
			<item component-name="$UI/system/components/justep/textarea/textarea" />
			<item component-name="$UI/system/components/justep/button/button" />
			<item component-name="$UI/system/components/justep/button/toggle" />
			<item component-name="$UI/system/components/justep/button/radio" />
			<item component-name="$UI/system/components/justep/button/checkbox" />
			<item component-name="$UI/system/components/justep/select/select" />
			<item component-name="$UI/system/components/justep/select/radioGroup" />
			<item component-name="$UI/system/components/justep/select/checkboxGroup" />
		</catalog>

		<catalog name="扩展" initial-state="open" design-view="excel"
			order="4">
			<item component-name="$UI/system/components/justep/labelEdit/labelInput" />
			<item component-name="$UI/system/components/justep/labelEdit/labelPassword" />
			<item component-name="$UI/system/components/justep/labelEdit/labelRange" />
			<item component-name="$UI/system/components/justep/labelEdit/labelOutput" />
			<item component-name="$UI/system/components/justep/labelEdit/labelSelect" />
			<item component-name="$UI/system/components/justep/labelEdit/labelTextarea" />
			<item component-name="$UI/system/components/justep/labelEdit/labelRadioGroup" />
			<item component-name="$UI/system/components/justep/labelEdit/labelCheckboxGroup" />
			<item component-name="$UI/system/components/justep/labelEdit/labelToggle" />
			<item component-name="$UI/system/components/justep/labelEdit/labelEdit" />
			<item component-name="$UI/system/components/justep/list/list" />
			<item component-name="$UI/system/components/justep/controlGroup/controlGroup" />
			<item component-name="$UI/system/components/justep/button/buttonGroup" />

		</catalog>

		<catalog name="高级" design-view="excel" order="5">
			<item component-name="$UI/system/components/justep/menu/menu" />
			<item component-name="$UI/system/components/justep/calendar/calendar" />
		</catalog>

		<catalog name="对话框" design-view="excel" order="6">
			<item component-name="$UI/system/components/justep/messageDialog/messageDialog" />
			<item component-name="$UI/system/components/justep/dialog/dialog" />
			<item component-name="$UI/system/components/justep/windowDialog/windowDialog" />
			<item component-name="$UI/system/components/justep/windowReceiver/windowReceiver" />
		</catalog>

		<catalog name="其它" order="9">
			<item component-name="$UI/system/components/justep/cordova/cordova" />
		</catalog>

		<catalog name="HTML" design-view="excel" order="10">
			<item component-name="input_button" />
			<item component-name="reset" />
			<item component-name="submit" />
			<item component-name="input" />
			<item component-name="file" />
			<item component-name="password" />
			<item component-name="checkbox" />
			<item component-name="radio" />
			<item component-name="hidden" />
			<item component-name="image" />
			<item component-name="textarea" />
			<item component-name="select" />
			<item component-name="horizontalRule" />
			<!-- htmltable -->
			<item component-name="table" />
			<item component-name="div" />
			<item component-name="span" />
			<item component-name="i" />
			<item component-name="label" />
			<item component-name="ul" />
			<item component-name="li" />
			<item component-name="h1" />
			<item component-name="h2" />
			<item component-name="h3" />
			<item component-name="h4" />
			<item component-name="h5" />
			<item component-name="a" />
			<item component-name="fieldset" />
			<item component-name="button" />
		</catalog>
	</toolbox>
	<depend-js>
		<item>$UI/system/core.min.js</item>
	</depend-js>
</reg-info>